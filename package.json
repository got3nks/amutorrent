{
  "name": "amutorrent-webapp-frontend",
  "version": "3.0.1",
  "description": "aMuTorrent Frontend Build",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./static/output.css --minify",
    "build:js": "node build.mjs",
    "build:profile": "npm run build:css && node build.mjs --profile",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "tailwindcss -i ./src/input.css -o ./static/output.css --watch",
    "watch:js": "node build.mjs --watch",
    "watch": "npm run watch:css & npm run watch:js"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.0"
  }
}
